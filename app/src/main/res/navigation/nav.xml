<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="edu.sjsu.android.vacationplanner.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_group"
        android:name="edu.sjsu.android.vacationplanner.GroupFragment"
        android:label="fragment_group"
        tools:layout="@layout/fragment_group" />

    <fragment
        android:id="@+id/navigation_budget"
        android:name="edu.sjsu.android.vacationplanner.BudgetFragment"
        android:label="fragment_budget"
        tools:layout="@layout/fragment_budget" />

    <fragment
        android:id="@+id/navigation_calendar"
        android:name="edu.sjsu.android.vacationplanner.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/navigation_saves"
        android:name="edu.sjsu.android.vacationplanner.SavesFragment"
        android:label="fragment_saves"
        tools:layout="@layout/fragment_saves" />
    <activity
        android:id="@+id/addMembersActivity"
        android:name="edu.sjsu.android.vacationplanner.group.AddMembersActivity"
        android:label="activity_add_members"
        tools:layout="@layout/activity_add_members" />

    <fragment
        android:id="@+id/navigation_voting"
        android:name="edu.sjsu.android.vacationplanner.VotingFragment"
        android:label="fragment_voting"
        tools:layout="@layout/fragment_voting" />
    <fragment
        android:id="@+id/navigation_votingGroup"
        android:name="edu.sjsu.android.vacationplanner.VotingGroup"
        android:label="fragment_voting_group"
        tools:layout="@layout/fragment_voting_group" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="edu.sjsu.android.vacationplanner.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <activity
        android:id="@+id/loginActivity"
        android:name="edu.sjsu.android.vacationplanner.login.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <activity
        android:id="@+id/mapsActivity"
        android:name="edu.sjsu.android.vacationplanner.MapsActivity"
        android:label="activity_maps"
        tools:layout="@layout/activity_maps" />


</navigation>